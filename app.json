{
  "expo": {
    "name": "MeetingAI",
    "slug": "MeetingAI",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#6200ee"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "此应用需要访问麦克风来录制会议音频",
        "NSSpeechRecognitionUsageDescription": "此应用需要语音识别权限来转录会议内容"
      }
    },
    "android": {
      "package": "com.charlo_o.MeetingAI",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6200ee"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "此应用需要访问麦克风来录制会议音频"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1a133f11-acaa-420c-a23e-0248040cede3"
      }
    }
  }
}
